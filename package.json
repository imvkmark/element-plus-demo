{
    "name": "demo-vue3",
    "version": "0.0.1",
    "private": false,
    "description": "A Vite2.x + Vue3 + TypeScript template starter",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "start:local": "vite --mode=self",
        "build": "vite build",
        "build:local": "vite build --mode=self",
        "build-tsc": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "format": "prettier --write ./src",
        "lint": "eslint ./src --ext .vue,.js,.ts",
        "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:imvkmark/demo-vue3.git"
    },
    "keywords": [
        "Vite",
        "Vue3",
        "TypeScript"
    ],
    "author": "XPoet",
    "license": "MIT",
    "homepage": "https://github.com/imvkmark/demo-vue3",
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^6.0.1",
        "@popjs/util": "^0.3",
        "@vant/touch-emulator": "^1.3.2",
        "axios": "^0.21.1",
        "better-mock": "^0.3.2",
        "crypto-js": "^4.0.0",
        "lodash-es": "^4.17.21",
        "vant": "^3.1.4",
        "vue": "^3.0.5",
        "vue-router": "^4.0.6",
        "vuex": "^4.0.0"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.0.2",
        "@types/lodash-es": "^4.17.4",
        "@types/node": "^16.4.13",
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "@vitejs/plugin-vue": "^1.2.1",
        "@vue/compiler-sfc": "^3.0.5",
        "eslint": "^7.24.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-vue": "^7.8.0",
        "less": "^4.1.1",
        "lint-staged": "^10.5.4",
        "typescript": "^4.1.3",
        "vite": "^2.1.5",
        "vite-plugin-style-import": "^0.9.2",
        "vue-tsc": "^0.0.15"
    }
}
