{
    "name": "element-plus-demo",
    "version": "0.0.1",
    "private": false,
    "description": "A Vite2.x + Vue3 + TypeScript template starter",
    "scripts": {
        "start:local": "vite --mode=self",
        "start:dev": "vite --mode=dev",
        "build": "vite build",
        "build:local": "vite build --mode=self",
        "build-tsc": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "format": "prettier --write ./src",
        "lint": "eslint ./src --ext .vue,.js,.ts",
        "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:imvkmark/element-plus-demo.git"
    },
    "keywords": [
        "Vite",
        "Vue3",
        "TypeScript"
    ],
    "author": "XPoet",
    "license": "MIT",
    "homepage": "https://github.com/imvkmark/element-plus-demo",
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^6.0.1",
        "@meilisearch/instant-meilisearch": "^0.13.0",
        "@popjs/util": "^0.3",
        "axios": "^1.4.0",
        "crypto-js": "^4.0.0",
        "element-plus": "^2.3.5",
        "lodash-es": "^4.17.21",
        "pinia": "^2.1.3",
        "poppy-util": "^0.0.4",
        "vue": "^3.3.4",
        "vue-instantsearch": "^4.10.0",
        "vue-router": "^4.2.1"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.0.2",
        "@types/lodash-es": "^4.17.7",
        "@types/node": "^18.15.0",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.6",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/compiler-sfc": "^3.3.4",
        "eslint": "^8.41.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-vue": "^9.13.0",
        "less": "^4.1.1",
        "typescript": "^5.0.4",
        "unplugin-vue-components": "^0.24.1",
        "vite": "^4.3.8",
        "vue-tsc": "^1.6.5"
    }
}
